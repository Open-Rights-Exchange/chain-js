  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "args": [
        "${fileBasename}",
        "--verbose",
        "-i",
        "--no-cache",
        "--watchAll"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Debug Algo Examples - applications.ts",
      "sourceMaps": true,
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/chains/algorand_1/examples/applications.ts",
      "cwd": "${workspaceRoot}/src/chains/algorand_1/examples",
      "env": {
        "TS_NODE_IGNORE": "false"
      },
    },
    {
      "name": "Debug Algo Examples - transfers.ts",
